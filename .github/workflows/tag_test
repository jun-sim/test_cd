name: Release Tag
on:
  push:
    tags:
      - dev-rc**
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v2
    - name: getTagName
      run: |
        echo ::set-output name=tag::${GITHUB_REF#refs/*/}
      id: tag_name 
    - name: printTagName
      run: 
        echo ${{steps.tag_name.outputs.tag}}
